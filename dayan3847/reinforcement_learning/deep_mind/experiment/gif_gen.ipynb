    "import numpy as np\n",
    "\n",
    "# Crea una lista de im√°genes PNG\n",
    "images = [imageio.v3.imread(f\"./frames/f{i}.png\") for i in np.linspace(1, 1000, 150, dtype=np.int64)]\n",
    "\n",
    "# Crea el GIF\n",
    "imageio.mimsave(\"output2.gif\", images)"
